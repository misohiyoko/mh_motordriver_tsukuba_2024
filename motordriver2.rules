
(rules PCB motordriver2
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 1985)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.3)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 200.0)
    (clearance 200.2)
    (clearance 100.0 (type smd_to_turn_gap))
    (clearance 200.2 (type smd))
    (clearance 200.2 (type "kicad_default"))
    (clearance 200.2 (type Imp50))
    (clearance 200.2 (type Pow0.44A))
    (clearance 200.2 (type Pow20A))
    (clearance 200.2 (type kicad))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_6000:4000_um"
    (shape
      (circle F.Cu 6000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 6000.0 0.0 0.0)
    )
    (attach off)
  )
  (padstack "Via[0-1]_5000:4000_um"
    (shape
      (circle F.Cu 5000.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 5000.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_5000:4000_um" "Via[0-1]_5000:4000_um" default
  )
  (via 
    "Via[0-1]_5000:4000_um-kicad_default" "Via[0-1]_5000:4000_um" "kicad_default"
  )
  (via 
    "Via[0-1]_5000:4000_um-Imp50" "Via[0-1]_5000:4000_um" Imp50
  )
  (via 
    "Via[0-1]_5000:4000_um-Pow0.44A" "Via[0-1]_5000:4000_um" Pow0.44A
  )
  (via 
    "Via[0-1]_5000:4000_um-Pow20A" "Via[0-1]_5000:4000_um" Pow20A
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_6000:4000_um" "Via[0-1]_6000:4000_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via 
    "Via[0-1]_6000:4000_um-kicad_default" "Via[0-1]_6000:4000_um" "kicad_default"
  )
  (via 
    "Via[0-1]_800:400_um-Imp50" "Via[0-1]_800:400_um" Imp50
  )
  (via 
    "Via[0-1]_6000:4000_um-Imp50" "Via[0-1]_6000:4000_um" Imp50
  )
  (via 
    "Via[0-1]_800:400_um-Pow0.44A" "Via[0-1]_800:400_um" Pow0.44A
  )
  (via 
    "Via[0-1]_6000:4000_um-Pow0.44A" "Via[0-1]_6000:4000_um" Pow0.44A
  )
  (via 
    "Via[0-1]_800:400_um-Pow20A" "Via[0-1]_800:400_um" Pow20A
  )
  (via 
    "Via[0-1]_6000:4000_um-Pow20A" "Via[0-1]_6000:4000_um" Pow20A
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (via_rule
    Imp50 "Via[0-1]_800:400_um-Imp50"
  )
  (via_rule
    Pow0.44A "Via[0-1]_800:400_um-Pow0.44A"
  )
  (via_rule
    Pow20A "Via[0-1]_6000:4000_um-Pow20A"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    "Net-(M1-+)" "/voltage_motor" VDD GND +5V "Net-(D1-K)" "Net-(D6-K)" "Net-(D7-DOUT)"
    "unconnected-(J2-Pad1)" "unconnected-(J2-Pad4)" "unconnected-(J2-Pad8)" /LED "Net-(Q1-G)" "Net-(Q2-G)" "Net-(Q3-G)" "Net-(Q4-G)"
    /GATEHA /GATELA /GATELB /GATEHB "unconnected-(D8-DOUT-Pad2)" "/PIC_UART_TX" "/PIC_UART_RX" "unconnected-(J2-Pad7)"
    "unconnected-(J2-Pad9)" "Net-(J3-Pin_1)" "Net-(J3-Pin_4)" "Net-(J3-Pin_5)" "unconnected-(J3-Pin_6-Pad6)" "unconnected-(J3-Pin_7-Pad7)" "unconnected-(J3-Pin_8-Pad8)" /SDA
    /SCL /IN2 /ISenseIN /MCLR* "Net-(U1B-+)" /IN1 /PGD /PGC
    /VSenseIN /INT "Net-(U2-HO)" "Net-(U2-LO)" "Net-(U4-HO)" "Net-(U4-LO)" /VSen
    (clearance_class "kicad_default")
    (via_rule "kicad_default")
    (rule
      (width 200.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Imp50
    (clearance_class Imp50)
    (via_rule Imp50)
    (rule
      (width 335.6)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Pow0.44A
    (clearance_class Pow0.44A)
    (via_rule Pow0.44A)
    (rule
      (width 400.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class Pow20A
    (clearance_class Pow20A)
    (via_rule Pow20A)
    (rule
      (width 20000.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)